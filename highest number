#include <stdio.h>
int main() { int i=0,n,max;
   int arr[100];
   printf("enter number ");
   scanf("%d",&n);
   printf(" enter the numbers list ");
   for(i=0;i<n;i++)
   {
   scanf("%d",&arr[i]);
   }
   printf("Displaying list: ");
   for(i=0;i<n;i++)
   {
  printf(" %d",arr[i]);
   }
   printf("\n");
   max=arr[0];
   for(i=1;i<n;i++)
   {
    if(max<arr[i]){
        max=arr[i];
    }
       
   }
   printf("%d",max);
    return 0;
}
